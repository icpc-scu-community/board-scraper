{
  "name": "newcomers-board-scraper",
  "version": "0.2.0",
  "main": "dist/index.js",
  "repository": {
    "url": "https://github.com/icpc-scu-community/board-scraper"
  },
  "scripts": {
    "start": "npm run submissions-parser:start",
    "dev": "npm run submissions-parser:dev",
    "submissions-parser:start": "node dist/index.js",
    "submissions-parser:dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "contests-parser:start": "node dist/sheetMetadata.js",
    "contests-parser:dev": "ts-node-dev --respawn --transpile-only src/sheetMetadata.ts",
    "build": "tsc",
    "lint": "eslint . --ext .js,.ts",
    "typecheck": "tsc --noEmit -p ."
  },
  "author": "Kerollos Magdy",
  "license": "ISC",
  "dependencies": {
    "chalk": "^4.1.2",
    "cheerio": "^1.0.0-rc.10",
    "dotenv": "^8.6.0",
    "got": "^11.8.2",
    "mongoose": "^6.0.11",
    "spinnies": "^0.5.1"
  },
  "devDependencies": {
    "@types/cheerio": "^0.22.30",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.4.1",
    "prettier": "^2.4.1",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.4.3"
  }
}
